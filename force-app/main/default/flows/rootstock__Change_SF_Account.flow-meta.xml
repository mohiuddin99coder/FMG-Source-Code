    <decisions>
        <name>Check_Financial_System_Interface</name>
        <label>Check Financial System Interface</label>
        <locationX>270</locationX>
        <locationY>242</locationY>
        <defaultConnector>
            <targetReference>Syconfig_Message_Screen</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Syconfig Financials Is &apos;FinancialForce&apos; or &apos;Accounting Seed&apos;</defaultConnectorLabel>
        <rules>
            <name>Record_Id_Is_null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>recordId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Vendor_Master_Selection_Screen</targetReference>
            </connector>
            <label>Record Id Is null</label>
        </rules>
        <rules>
            <name>Syconfig_Financials_Is_Not_FinancialForce_or_Accounting_Seed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_System_Configuration.rstk__syconfig_financials__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>FinancialForce</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_System_Configuration.rstk__syconfig_financials__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Accounting Seed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Vendor_Master</targetReference>
            </connector>
            <label>Syconfig Financials Is Not &apos;FinancialForce&apos; or &apos;Accounting Seed&apos;</label>
        </rules>
    </decisions>
